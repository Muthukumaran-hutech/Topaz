<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/orderconfirmation_layout"
    tools:context=".Activities.OrderConfirmation">


    <ImageView
        android:id="@+id/rectangle_269"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/eclipse_46"
        android:layout_width="73dp"
        android:layout_height="73dp"

        android:layout_marginTop="250dp"
        android:src="@drawable/ellipse_46"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/tick"
        android:layout_width="35dp"
        android:layout_height="18dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/vector_85"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/eclipse_46"
        android:layout_marginTop="10dp"
        android:padding="@dimen/_5sdp"
        android:text="@string/thank_you"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"

        android:textStyle="bold"/>

    <TextView
        android:id="@+id/txtreuest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/your_request_has_been_sent"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/torequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/to"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/txtreuest"
        app:layout_constraintTop_toBottomOf="@+id/txtreuest" />


    <TextView
        android:id="@+id/companynm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:text="@string/gold_wood_industries_"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/torequest"
        app:layout_constraintTop_toBottomOf="@+id/txtreuest" />
    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        app:layout_constraintTop_toBottomOf="@id/companynm"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/white"
        android:text="Back to home"
        android:padding="6dp"
        android:textAllCaps="false"
        android:id="@+id/goHomeButton"
        android:visibility="gone"
        android:background="@drawable/blue_button_rounded_corner"
        app:layout_constraintEnd_toEndOf="parent"/>



